  "scripts": {
    "husky": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \\\\\\\\\\\\\\\"libs/**/*.ts\\\\\\\\\\\\\\\" \\\\\\\\\\\\\\\"apps/**/*.ts\\\\\\\\\\\\\\\"",
    "start:gateway": "nest start gateway",
    "start:service": "nest start service",
    "start:worker": "nest start worker",
    "start:admin": "nest start admin",
    "start:dev:gateway": "nest start gateway --watch",
    "start:dev:service": "nest start service --watch",
    "start:dev:worker": "nest start worker --watch",
    "start:dev:admin": "nest start admin --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod:gateway": "node dist/apps/game/src/main --max-old-space-size=4096",
    "start:prod:service": "node dist/apps/service/src/main --max-old-space-size=2048",
    "start:prod:worker": "node dist/apps/worker/src/main --max-old-space-size=2048",
    "start:prod:admin": "node dist/apps/admin/src/main --max-old-space-size=2048",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "dev:check": "tsc --noEmit --project tsconfig.build.json",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test": "jest --logHeapUsage --workerMemoryThreshold=1G --workerMemoryThreshold=80% --maxWorkers=30%",
    "test:coverage": "jest --coverage --logHeapUsage --workerMemoryThreshold=1G --workerMemoryThreshold=80% --maxWorkers=30%",
    "test:e2e": "jest --logHeapUsage --workerMemoryThreshold=1G --workerMemoryThreshold=80% --maxWorkers=30% --config ./jest-e2e.config.json",
    "test:e2e:watch": "jest --watch --config ./jest-e2e.config.json",
    "migration": "ts-node -r tsconfig-paths/register apps/gateway/src/migration.ts",
    "migration:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register apps/gateway/src/migration.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register apps/gateway/src/migration.ts revert",
    "migration:create": "ts-node libs/utils/db/migration-create.ts"
  },
